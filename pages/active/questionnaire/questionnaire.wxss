button::after {
  border-radius: 0;
}

button {
  border-radius: 0;
}

.startup-page {
  text-align: center;
}

.startup-page .til {
  padding-top: 70rpx;
  display: flex;
  flex-direction: column;
  font-size: 60rpx;
  color: #4a4a4a;
  letter-spacing: 0;
  text-align: center;
  line-height: 76rpx;
}

.startup-page .bg-pic {
  margin-top: 182rpx;
}

.startup-page .bg-pic image {
  width: 394rpx;
  height: 326rpx;
}

.bottom-btn {
  text-align: center;
  background: #fff;
  height: 110rpx;
  position: fixed;
  z-index: 9999;
  bottom: 0;
  left: 0;
  right: 0;
  box-shadow: 0px 0 32rpx rgba(0, 0, 0, 0.12);
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
}

.bottom-btn .btn {
  position: absolute;
  top: 12rpx;
  left: 30rpx;
  width: 690rpx;
  line-height: 88rpx;
  background: #f33;
  font-size: 36rpx;
  color: #fff;
  letter-spacing: 0.22px;
  border: 0;
}

.question-all {
  padding-bottom: 90rpx;
}

.question-all .bottom-btn {
  background: #fff;
  height: 110rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  box-shadow: 0px 0 32rpx rgba(0, 0, 0, 0.12);
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
}

.question-all .bottom-btn .btn {
  position: absolute;
  top: 12rpx;
  left: 30rpx;
  width: 690rpx;
  line-height: 88rpx;
  font-size: 36rpx;
  color: #fff;
  letter-spacing: 0.22px;
  text-align: center;
  background: rgba(255, 51, 51, 0.30);
}

.question-all .bottom-btn .redBtn {
  background: #f33;
}

.userInfo-wrap {
  padding: 40rpx 56rpx;
}

.userInfo-wrap .title {
  display: flex;
  flex-direction: column;
  font-size: 44rpx;
  color: #333;
  letter-spacing: 0;
  line-height: 60rpx;
  margin-bottom: 172rpx;
  margin-top: 80rpx;
}

.user-content {
  margin-bottom: 56rpx;
  display: flex;
  flex-direction: row;
}

.user-content input {
  font-size: 32rpx;
  color: #333;
  line-height: 70rpx;
  width: 480rpx;
  height: 70rpx;
}

.user-content .gender {
  display: flex;
  flex-direction: row;
}

.user-content .gender text {
  width: 200rpx;
  line-height: 64rpx;
  background: #f4f4f4;
  border-radius: 4rpx;
  font-size: 30rpx;
  color: #4a4a4a;
  letter-spacing: 0;
  text-align: center;
}

.user-content .gender .man {
  margin-right: 46rpx;
}

.user-content .gender .active {
  background: #fcd1cc;
  color: #f33;
}

.user-content .info-til {
  font-size: 48rpx;
  color: #333;
  margin-right: 54rpx;
}

.success-model {
  width: 100%;
  height: 100%;
  background: #fff;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.img-box {
  width: 750rpx;
  padding: 300rpx 0 100rpx;
  text-align: center;
}

.success-model .iconfont {
  font-size: 200rpx;
  color:#ff3333;
}

.success-model .text {
  width: 750rpx;
  font-size: 44rpx;
  color: #4a4a4a;
}

.success-model .text text {
  width: 750rpx;
  display: inline-block;
  text-align: center;
}

.modal-wrap {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.3);
  z-index: 9999;
  display: flex;
  align-items: center;
  justify-content: center;
}

.modal-wrap .modal-conten {
  width: 540rpx;
  height: 280rpx;
  background: #f5f5f5;
  border-radius: 24rpx;
}

.modal-wrap .modal-conten .title {
  text-align: center;
  font-size: 40rpx;
  color: #030303;
  letter-spacing: -0.96rpx;
  margin-top: 84rpx;
}

.modal-wrap .modal-conten .btn {
  border-top: 2rpx solid #dedede;
  margin-top: 51rpx;
  line-height: 90rpx;
  font-size: 34rpx;
  color: #007aff;
  letter-spacing: 0;
  text-align: center;
}

.modal-wrap .modal-conten .btn text {
  display: inline-block;
  width: 50%;
  box-sizing: border-box;
}

.modal-wrap .modal-conten .btn .cancel {
  border-right: 2rpx solid #dedede;
}
