.container {
    width: 700rpx; margin-left: 25rpx; height: 100%; position: relative;
}
.container .xlTitle {
    width: 100%; height: 100rpx; line-height: 100rpx; position: relative;
    font-size: 24rpx; color: #B2B2B2; text-align: center; margin-bottom: 80rpx;
}
.container .xlTitle::after {
    content: ''; width: 80%; position: absolute; left: 10%; top: 50rpx; z-index: -1;
    height: 1px; background: #EEE;
}
.container .xlTitle text {
    padding: 0 8px; background: #FFF;
}
.xlSubmitBtn {
    width: 100%; background: #1AAD19; height: 110rpx; line-height: 110rpx; 
    font-size: 36rpx; color: #FFF; border-radius: 8rpx; position: relative; text-align: center;
}
.xlSubmitBtn image {
    width: 50rpx; height: 50rpx; margin-right: 24rpx; vertical-align: middle;
}