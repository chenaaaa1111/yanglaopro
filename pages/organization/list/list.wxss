@import "../../../styles/list-item.wxss";
page {
  position: relative;
}

.container {
  font-size: 24rpx;
  height: auto;
  overflow: auto;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.hidden {
  display: none;
}

.show {
  display: block;
}

.no-house {
  width: 750rpx;
  font-size: 38rpx;
  text-align: center;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  -webkit-transform: translate(-50%, -50%);
}

.diasp-bg {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.6);
  position: fixed;
  z-index: 8888;
  top: 0;
}

.sort-btn {
  width: 150rpx;
  height: 150rpx;
  position: fixed;
  bottom: 150rpx;
  right: 10rpx;
}

.sort-btn image {
  width: 100%;
  height: 100%;
}

.head-nav {
  position: fixed;
  top: 0;
  background: #fff;
  width: 100%;
  z-index: 9999;
  box-shadow: 0 0 16px 0 rgba(0, 0, 0, 0.16);
}

.search-tabbar {
  padding: 24rpx 0 0 32rpx;
  display: flex;
  flex-direction: row;
}

.search {
  width: 600rpx;
  height: 72rpx;
  padding-left: 78rpx;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  background: #f2f2f2;
  color: #333;
  font-size: 26rpx;
  margin-right: 15rpx;
}

.tabBar-4 text {
  display: inline-block;
  width: 104rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.box-st {
  position: relative;
}

.box-st .icon-sousuo {
  position: absolute;
  font-size:34rpx;
  top:18rpx;
  left: 34rpx;
  color:#b7b7b7;
}

.btn-area {
  margin: 8rpx 0 0 20rpx;
}

.btn-area .icon-ditu {
  font-size: 54rpx;
  color:#4a4a4a;
}

.sort-list {
  display: flex;
  flex-direction: row;
  position: relative;
}

.sort-list .sort {
  width: 25%;
  text-align: center;
  line-height: 90rpx;
  font-size: 32rpx;
}

.sort-list .current {
  color: #ff4848;
}

.sort-list .sort .icon-sanjiao{
  font-size:40rpx;
  color:#b5b5b5;
}
.wrap-pull-down {
  width: 100%;
  position: absolute;
  top: 186rpx;
  background: #fff;
  font-size: 32rpx;
}

.areas {
  display: flex;
  flex-direction: row;
  background: #f8f8f8;
}

.areas .first-area {
  width: 250rpx;
  text-align: center;
  line-height: 88rpx;
  border-bottom: 2rpx solid #f6f6f6;
}

.areas .first-area .area-item {
  border-top: 2rpx solid #f6f6f6;
}

.areas .first-area .current {
  background: #fff;
  color: #ff4848;
}

.areas .area-name {
  width: 500rpx;
  text-align: center;
  line-height: 88rpx;
  background: #fff;
}

.areas .area-name .area-item {
  border-top: 2rpx solid #f6f6f6;
}

.wrap-pull-down .current {
  color: #ff4848;
}

.price-list {
  width: 100%;
  line-height: 88rpx;
  border-top: 2rpx solid #f6f6f6;
  text-align: center;
  font-size: 32rpx;
}

.price-list text {
  width: 100%;
  display: inline-block;
  text-align: center;
}

.care-type {
  border-top: 2rpx solid #f6f6f6;
}

.care-type .itemType {
  width: 100%;
  line-height: 88rpx;
  border-bottom: 2rpx solid #f6f6f6;
  text-align: center;
  font-size: 32rpx;
}

.care-type .itemType label {
  display: inline-block;
  width: 100%;
  height: 100%;
  position: relative;
}

.care-type .itemType label .normal {
  display: none;
}

.care-type .active {
  color: #ff4848;
}

.care-type .active label .checked-icon {
  width: 36rpx;
  height: 36rpx;
  position: absolute;
  top: 30rpx;
  right: 40rpx;
  display: block;
}

.care-type .confirm-btn {
  margin: 24rpx auto;
  width: 690rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 32rpx;
  background: #f33;
  color: #fff;
}

.more-wrap-height {
  height: 100%;
}

.more-wrap {
  border-top: 2rpx solid #eee;
  background: #fff;
  position: relative;
  height: 1800rpx;
}

.more-wrap .more-item {
  border-bottom: 2rpx solid #eee;
  padding: 0 0 20rpx 30rpx;
}

.more-wrap .more-item .title {
  font-size: 40rpx;
  line-height: 40rpx;
  color: #333;
  margin-top: 26rpx;
  margin-bottom: 38rpx;
}

.more-wrap .more-item .context text {
  background: #f4f4f4;
  border-radius: 4rpx;
  display: inline-block;
  width: 200rpx;
  line-height: 64rpx;
  text-align: center;
  font-size: 32rpx;
  color: #6a6a6a;
  margin-right: 40rpx;
  margin-bottom: 30rpx;
}

.medical-types {
  padding: 0 0 20rpx 30rpx;
}

.medical-types .types {
  padding: 32rpx 0 0;
  display: flex;
  flex-direction: row;
  box-sizing: border-box;
}

.medical-types .types .item {
  width: 33.333%;
}

.medical-types .types .title {
  display: inline-block;
  font-size: 32rpx;
  color: #333;
  margin-bottom: 32rpx;
}

.medical-types .status {
  display: flex;
  flex-direction: row;
}

.medical-types .status {
  background: #f4f4f4;
  border-radius: 4rpx;
  display: inline-block;
  width: 200rpx;
  line-height: 64rpx;
  text-align: center;
  font-size: 32rpx;
  color: #6a6a6a;
  padding: 0;
  margin-bottom: 22rpx;
}

.more-wrap-height .more-wrap .more-item .active {
  background: #fcd1cc;
  color: #f33;
}

.more-wrap-height .more-wrap .medical-types .active {
  background: #fcd1cc;
  color: #f33;
}

.more-scroll {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 140rpx;
  overflow: auto;
}

.more-wrap-height .submit-btn {
  width: 100%;
  display: flex;
  flex-direction: row;
  padding: 26rpx 0 26rpx 30rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background: #fff;
  border-top: 2rpx solid #eee;
}

.more-wrap-height .submit-btn text {
  display: inline-block;
  width: 330rpx;
  line-height: 80rpx;
  border: 2rpx solid #f33;
  box-sizing: border-box;
  text-align: center;
  font-size: 32rpx;
  color: #f33;
}

.more-wrap-height .submit-btn .confirm {
  background: #f33;
  color: #fff;
  margin-left: 30rpx;
}

.lists-items {
  margin-top: 194rpx;
}

/* .scroll-view {
  height: 1038rpx;
} */

.sort-wrap {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  background: rgba(0, 0, 0, 0.6);
  z-index: 99999;
}

.sort-wrap .sort-methods {
  width: 100%;
  position: absolute;
  bottom: 0;
  background: #fff;
}

.sort-wrap .sort-methods .sort-item .active {
  color: #e2594a;
}

.sort-wrap .sort-methods .method {
  line-height: 88rpx;
  border-bottom: 2rpx solid #f6f6f6;
  font-size: 32rpx;
  color: #000;
  text-align: center;
  position: relative;
}

.sort-wrap .sort-methods .method .sort-token {
  font-size: 47rpx;
  position: absolute;
  top: 0;
  right: 190rpx;
  color:#b2b2b2;
}
