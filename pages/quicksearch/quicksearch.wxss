/* pages/quickSearch/index.wxss */
@import "./../../styles/common.wxss";
text,view,button {
  box-sizing: border-box;
}
.coner{
  padding:60rpx 0rpx 100rpx 0rpx;
  font-size: 32rpx;
}
.title{
  margin-bottom: 30rpx;
  font-size: 40rpx;
  font-weight: 900;
}
.coner text{
  background: #F4F4F4;
  border-radius: 4rpx;
  margin-right: 39rpx; 
  margin-bottom: 22rpx;
  display: inline-block;
  text-align: center;
  color: #666666;
  width: 200rpx;
  font-size: 30rpx;
   padding:  10rpx auto;
   height: 64rpx;
   line-height: 64rpx;
}
.coner text.active{
  background: #fcd2cd;
  color: #FF3333;
}
.content{
  padding-bottom:40rpx;
border-bottom:2rpx solid #EEEEEE;
margin-bottom:60rpx;
padding-left:34rpx; 
}
.nobtom{
  border-bottom: none;
}
.fotom{
  position: fixed;
  bottom: 0rpx; 
  padding-bottom: 30rpx 30rpx 10rpx   30rpx;  
  background: #fff;
  padding: 30rpx 30rpx;
  width: 100%;
  border-top: 2rpx solid #eee;
}
.fotom button{
  width: 330rpx;
  display: inline-block;
  font-size: 40rpx;
}
.ml20{
  margin-left: 20rpx;
  
}
.refrush{
  border: 2rpx solid #FF3333;
  color: #FF3333;
  background: #fff;
}
.mackSure{
  background:  #FF3333;
  color: #fff;
  margin-left: 62rpx;
  position: absolute;right: 30rpx;
}
.fotom button{
  margin-top:10rpx; 
  border-radius: 0;
  outline: none;
}
button::after {
  border: 0;
} 

