.xlTabbarBox {
    width: 100%; height: 44px; position: fixed; bottom: 0; left: 0; display: flex; box-shadow: -2px -4px 6px #CCC;
    z-index: 10000; background: #FFF;
}
.xlTabbarBox .xlList {
    width: 50%; box-sizing: border-box; height: 44px; line-height: 44px; text-align: center; position: relative;
}
.xlTabbarBox .xlList.xlActive .xlText{
    color: #FF3333;
}
.xlTabbarBox .xlList .xlImgBox {
    width: 16px; height: 18px; position: absolute; top: 6px; left: 50%; margin-left: -8px; overflow: hidden;
}
.xlTabbarBox .xlList .xlImgBox image {
    width: 100%; height: 100%; float: left;
}
.xlTabbarBox .xlList .xlText {
    height: 20px; line-height: 20px; text-align: center; margin-top: 22px; font-size: 10px;
}