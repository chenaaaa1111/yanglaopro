@import "../../../styles/msg-board.wxss";
.container {
  font-size: 32rpx;
  color: #000;
  background: #f4f4f4;
}

.tags {
  margin: 36rpx 0 50rpx 36rpx;
}

.tags .tag {
  display: inline-block;
  width: 72rpx;
  height: 30rpx;
  font-size: 22rpx;
  color: #74b4eb;
  text-align: center;
  line-height: 32rpx;
  border: 1px solid #74b4eb;
  margin-right: 14px;
}

.container .lump {
  padding-right: 34rpx;
  background: #fff;
}

.container .lump-name-price .house-name {
  padding-left: 36rpx;
  padding-top: 40rpx;
}

.container .lump-name-price .house-name .name {
  font-weight: bold;
  font-size: 48rpx;
  color: #333;
  letter-spacing: 0.19px;
  margin-right: 56rpx;
  vertical-align: middle;
}

.container .lump-name-price .house-name .state {
  font-size: 24rpx;
  color: #fff;
  letter-spacing: 0;
  background: #f62f5e;
  padding: 4rpx 6rpx;
  display: inline-block;
  vertical-align: middle;
}

.container .lump-name-price .price {
  margin: 30rpx 0 10rpx 36rpx;
  position: relative;
}

.container .lump-name-price .price .price-num {
  font-size: 48rpx;
  color: #f33;
  letter-spacing: 0.24px;
  font-weight: bold;
}

.container .lump-name-price .price .units {
  font-size: 28rpx;
  letter-spacing: 0.24px;
  color: #f33;
  margin-left: 24rpx;
}

button[plain] {
  border: 0;
}

.container .lump-name-price .price .share {
  width: 46rpx;
  height: 44rpx;
  float: right;
  margin-top: 12rpx;
  margin-right: 20rpx;
}

.cost-explain {
  font-size: 24rpx;
  color: #9b9b9b;
  margin-left: 36rpx;
  line-height: 44rpx;
}

.container .lump-name-price .caretype {
  display: flex;
  flex-direction: row;
  color: #4a4a4a;
  background: #f2f2f2;
  /* line-height: 120rpx; */
  padding: 56rpx 0 36rpx 36rpx;
}

.container .lump-name-price .caretype .type {
  width: 520rpx;
}

.container .lump-name-price .caretype .type text {
  display: inline-block;
  margin-left: 20rpx;
  margin-bottom: 20rpx;
}

.container .lump-name-price .introduce {
  display: flex;
  flex-direction: column;
  margin-top: 42rpx;
  padding-left: 36rpx;
  padding-bottom: 50rpx;
}

.container .lump-name-price .introduce .til {
  font-size: 36rpx;
  color: #000;
  line-height: 56rpx;
  font-weight: bold;
  margin-bottom: 32rpx;
}

.container .lump-name-price .introduce .con {
  width: 100%;
  color: #000;
  line-height: 56rpx;
  text-align: justify;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  -ms-flex-pack: justify;
  justify-content: space-between;
}

.container .lump-wrap {
  display: flex;
  flex-direction: column;
  margin-top: 24rpx;
  padding: 46rpx 32rpx 25px;
  box-sizing: border-box;
}

.container .housetypes {
  padding-right: 0;
  padding-bottom: 30rpx;
}

.signIn .welcom {
  font-size: 56rpx;
  margin: 0 auto;
  text-align: center;
}

.signIn .cont {
  font-size: 32rpx;
  margin: 20rpx auto 40rpx;
  text-align: center;
  color: #999;
  display: flex;
  flex-direction: column;
  line-height: 48rpx;
}

.signIn .signIn-btn {
  /* width: 356px;
  height:149px; */
  width: 444rpx;
  height: 242rpx;
  margin: 0 auto;
  position: relative;
}

.signIn .signIn-btn image {
  width: 100%;
  height: 100%;
  margin: 0 auto;
}

.signIn .signIn-btn .word {
  text-align: center;
  color: #fff;
  font-size: 48rpx;
  position: absolute;
  top: 64rpx;
  left: 176rpx;
}

.container .lump-wrap .h3 {
  font-family: PingFangSC-Medium;
  font-size: 40rpx;
  color: #000;
  margin-bottom: 32rpx;
  font-weight: bold;
}

.container .lump-wrap .info-box {
  display: flex;
  flex-direction: row;
  margin-bottom: 20rpx;
}

.container .lump-wrap .info-box .content {
  width: 520rpx;
  line-height: 56rpx;
}

.xlMap {
  width: 100%;
  height: 320rpx;
}

.container .lump-wrap .h4 {
  color: #999;
  line-height: 56rpx;
  width: 120rpx;
  /* display: inline-block;
  text-align: justify;
  text-justify: distribute-all-lines;
  text-align-last: justify;
  vertical-align: top; */
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.container .lump-wrap .colon {
  color: #999;
  line-height: 56rpx;
  padding-right: 16rpx;
  padding-left: 8rpx;
}

.container .lump-wrap .info-box .content .address-map {
  overflow: hidden;
  width: 500rpx;
  height: 320rpx;
  margin-top: 14rpx;
}

.container .lump-wrap .room-types {
  /* max-width: 620rpx; */
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
  flex-wrap: wrap;
  margin: 0 auto;
}

.container .lump-wrap .room-types .room {
  margin-bottom: 30rpx;
  font-size: 44rpx;
  color: #333;
  text-align: center;
}

.xlCollectionBox {
  width: 54rpx;
  height: 54rpx;
}

.container .lump-wrap .room-types .room .roomtype-til {
  font-size: 44rpx;
  color: #333;
  display: inline-block;
  line-height: 60rpx;
  padding: 0 38rpx;
}

.container .lump-wrap .room-types .room .valLine {
  border-right: 2rpx solid #dfdfdf;
}

.container .lump-wrap .room-types .room .space {
  margin-top: 18rpx;
}

.container .lump-wrap .room-types .room .space text {
  color: #9292af;
}

.container .lump-wrap .shadow-box {
  border: 2rpx solid rgba(219, 223, 227, 0.30);
  box-shadow: 0 10rpx 24rpx 0 rgba(217, 226, 233, 0.50);
  border-radius: 12rpx;
  padding: 30rpx 40rpx;
  margin-bottom: 30rpx;
}

.container .lump-wrap .shadow-box-pink {
  background: #fff;
  border: 2rpx solid rgba(219, 223, 227, 0.30);
  box-shadow: 0 10rpx 24rpx 0 rgba(255, 51, 51, 0.10);
  border-radius: 12rpx;
}

.container .lump-wrap .boldTil {
  font-size: 36rpx;
  color: #485465;
  position: relative;
  margin-top: 10px;
}

.container .lump-wrap .boldTil .num {
  float: right;
  right: 0;
}

.cost-breakdown .top {
  display: flex;
  flex-direction: column;
  font-size: 24rpx;
  color: #a8b4c4;
}

.cost-breakdown .top .operat-instruction {
  display: flex;
  flex-direction: row;
  position: relative;
  margin-top: 16rpx;
}

.cost-breakdown .top .operat-instruction .em {
  text-align: right;
  letter-spacing: 0.4rpx;
  position: absolute;
  right: 0;
}

.shadow-box .price-em {
  position: relative;
  margin-bottom: 40rpx;
}

.shadow-box .price-em .em {
  text-align: right;
  letter-spacing: 0.4rpx;
  position: absolute;
  right: 0;
  font-size: 24rpx;
  color: #a8b4c4;
}

.lump-wrap .tab-col {
  display: flex;
  flex-direction: column;
  margin-bottom: 34rpx;
  font-size: 36rpx;
}

.lump-wrap .tab-col .td {
  color: #999;
  margin-top: 10rpx;
}

.container .zhu {
  font-size: 32rpx;
  display: flex;
  flex-direction: row;
  color: #666;
  line-height: 52rpx;
  padding-bottom: 120rpx;
  margin-bottom: 60rpx;
}

.toHome {
  width: 120rpx;
  height: 120rpx;
  position: fixed;
  bottom: 150rpx;
  right: 10rpx;
}

.toHome image {
  width: 100%;
  height: 100%;
}

.container .user-msg-wrap .h3 {
  margin-bottom: 0;
}

.not-ava {
  color: #666;
  margin: 60rpx auto 18rpx;
}

.msg-btn {
  margin: 12rpx 0;
}

.container .user-msg-wrap, .noline .user-msg-info {
  border: 0;
}

.msg-btn .btn {
  border-radius: 16rpx;
  font-size: 36rpx;
  line-height: 100rpx;
  text-align: center;
  margin-top: 32rpx;
}

.more-msg-btn {
  background: #ededed;
  color: #4a4a4a;
}

.write-msg-btn {
  background-image: linear-gradient(45deg, #ff7e7b 0%, #ffb686 100%);
  box-shadow: 0 10px 10px 0 rgba(255, 116, 110, 0.30);
  color: #fff;
}

.phone-dys {
  display: flex;
  flex-direction: row;
  width: 100%;
  background: #fff;
  height: 110rpx;
  padding: 12rpx 30rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  box-shadow: 0px 0 32rpx rgba(0, 0, 0, 0.12);
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
}

.dys {
  width: 360rpx;
  display: flex;
  flex-direction: row;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
}

.dys-li {
  width: 33.333%;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
  font-size: 28rpx;
  color: #4a4a4a;
}

.dys .dys-li .cover {
  width: 100%;
  height: 100%;
}

.dys-li .share-btn {
  width: 100%;
  height: 100%;
  opacity: 0;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 9999;
}

.shareDetail {
  width: 115rpx;
  height: 104rpx;
  position: absolute;
  top: 1rpx;
  left: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.shareDetail .icon-fenxiang {
  font-size: 50rpx;
  color:#ff3333;
}

/* .shareDetail .share {
  margin-top: 10rpx;
} */

.callphone {
  margin-left: 40rpx;
}

.callphone .btn {
  display: inline-block;
  width: 330rpx;
  height: 88rpx;
  background: #f33;
  font-size: 32rpx;
  color: #fff;
  letter-spacing: 0.38rpx;
  text-align: center;
  vertical-align: middle;
  line-height: 88rpx;
}

.grade-wrap {
  text-align: center;
  margin-bottom:98rpx;
  padding-top:30rpx;
}

.grade-wrap .score {
  font-size: 120rpx;
  color: #fd7015;
  letter-spacing: 0;
  text-shadow: 0 12rpx 36rpx rgba(255, 51, 51, 0.35);
  font-weight: 600;
}

.grade-wrap .total-score {
  font-size: 36rpx;
  color: #9b9b9b;
  letter-spacing: 0;
  text-align: center;
  line-height: 40rpx;
  margin-top:6rpx;
}

.grade-wrap .grade-num {
  font-size: 36rpx;
  color: #4a4a4a;
  letter-spacing: 0;
  text-align: center;
  line-height: 40rpx;
  margin-top:32rpx;
}
